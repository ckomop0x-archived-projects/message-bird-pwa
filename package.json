{
  "name": "message-bird-app",
  "version": "1.1.0",
  "description": "Message Bird Application",
  "private": true,
  "scripts": {
    "buildserver": "tsc",
    "buildclient": "rm -rf dist/client && NODE_ENV='production' webpack -p",
    "client": "rm -rf dist/client && webpack --mode=development && npm run startdev",
    "deploy:github": "gh-pages -d dist",
    "devserver": "nodemon",
    "heroku-prebuild": "npm install && npm run buildclient && npm run buildserver",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:write": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "lintcss": "stylelint 'src/**/*.ts'",
    "lock": "npm shrinkwrap --no-optional",
    "prettier": "prettier --write 'src/**/*.{ts,tsx,json}'",
    "start": "node ./dist/server/index.js",
    "startdev": "node_modules/.bin/webpack-dev-server",
    "test": "npm run lint && npm run lintcss && npx jest --no-cache --no-watchman",
    "test:watch": "npx jest --watch",
    "test:coverage": "npx jest --coverage"
  },
  "author": "paul.klochkov@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@loadable/component": "^5.2.1",
    "@types/react-loadable": "^5.4.2",
    "autoprefixer": "^9.2.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "css-loader": "^1.0.0",
    "dotenv-webpack": "^1.5.7",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "express": "^4.16.4",
    "html-webpack-plugin": "^3.2.0",
    "messagebird": "^2.2.0",
    "moment": "^2.22.2",
    "moment-parseformat": "^3.0.0",
    "nprogress": "^0.2.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-loadable": "^5.5.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-websocket": "^2.0.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "styled-components": "^4.0.2",
    "styled-components-ts": "0.0.14",
    "typescript": "^3.2.2",
    "ts-loader": "^5.2.2",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-hot-middleware": "^2.24.3",
    "workbox-webpack-plugin": "^3.6.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "mini-css-extract-plugin": "^0.4.4",
    "typings-for-css-modules-loader": "^1.7.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.4",
    "@types/enzyme": "^3.1.14",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.7",
    "@types/nprogress": "0.0.29",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-router": "^4.4.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-test-renderer": "^16.0.3",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/styled-components": "^4.0.3",
    "dotenv": "^6.2.0",
    "gh-pages": "^2.0.1",
    "husky": "^1.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-styled-components": "^6.2.2",
    "lint-staged": "^8.1.0",
    "nodemon": "^1.18.6",
    "postcss-custom-media": "^7.0.7",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.15.2",
    "react-test-renderer": "^16.6.0",
    "stylelint": "^9.9.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-import-rules": "^0.3.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typed-css-modules": "^0.3.6"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "test/coverage",
    "coverageReporters": [
      "cobertura",
      "html"
    ],
    "collectCoverageFrom": [
      "src/scripts/*.{ts,tsx}",
      "src/scripts/**/*.{ts,tsx}",
      "!src/scripts/*.d.ts",
      "!src/scripts/**/*.d.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "setupFiles": [
      "<rootDir>/test-shim.js",
      "<rootDir>/test-setup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx|js|jsx)$": "<rootDir>/test-preprocessor.js"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "^.+\\.(d.ts)$"
    ],
    "testRegex": "(/test/unit/.*|\\.(spec))\\.(ts|tsx|js|jsx)$"
  }
}
